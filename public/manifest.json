{
  "name": "GraphQL Network Inspector",
  "version": "1.0",
  "description": "Simple and clean network inspector for GraphQL",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 3,
  "permissions": [],
  "devtools_page": "devtools/devtools.html",
  "content_scripts": [
    {
      "matches": ["http://localhost:3000/draft?*"],
      "js": ["contentScript.js"]
    }
  ]
}
